<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Number_of_Employees_Range</fullName>
    <active>true</active>
    <description>Ensures Number of Employees is between 10 and 100,000</description>
    <errorConditionFormula>OR(
    NumberOfEmployees &lt; 10,
    NumberOfEmployees &gt; 100000
)</errorConditionFormula>
    <errorDisplayField>NumberOfEmployees</errorDisplayField>
    <errorMessage>Number of Employees must be between 10 and 100,000</errorMessage>
</ValidationRule>